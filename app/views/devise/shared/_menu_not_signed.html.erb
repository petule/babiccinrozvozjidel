<li><%= link_to t("devise.action.sign_in").capitalize, new_user_session_path %></li>
<% if devise_mapping.registerable? %>
	<li><%= link_to t("devise.action.sign_up").capitalize, new_user_registration_path %></li>
<% end %>
<% if devise_mapping.recoverable? %>
	<li><%= link_to t("devise.action.reset_password").capitalize, new_user_password_path %></li>
<% end %>